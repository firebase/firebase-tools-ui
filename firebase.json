{
  "//": "This is needed for tests, as storage emulator requires rules",
  "storage": {
    "rules": "storage.rules"
  },
  "firestore": {},
  "emulators": {
    "firestore": {
      "port": 8081
    },
    "storage": {
      "port": 9199
    },
    "ui": {
      "enabled": true
    },
    "auth": {
      "port": 9099
    },
    "functions": {
      "port": 5001
    },
    "hosting": {
      "port": 5000
    },
    "database": {
      "port": 9000
    },
    "pubsub": {
      "port": 8085
    },
    "scheduled": {
      "port": 8086
    }
  },
  "extensions": {
    "storage-resize-images": "firebase/storage-resize-images@0.1.35",
    "firestore-stripe-payments": "stripe/firestore-stripe-payments@0.3.3"
  }
}
